# Environment
ENV=development

# Google Drive API Configuration
GOOGLE_DRIVE_CREDENTIALS_FILE=deployment/credentials/google_service_account.json

# Logging Configuration
LOG_LEVEL=DEBUG

# MCP Server Configuration
MCP_SERVER_NAME=MindMup Google Drive Server
MCP_SERVER_VERSION=0.1.0

# Setting
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=30
CACHE_ENABLED=true
CACHE_TTL=300